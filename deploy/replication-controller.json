{
  "apiVersion": "v1",
  "kind": "ReplicationController",
  "metadata": {
    "generateName": "kyrene-"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "service_name": "kyrene",
      "version": "v3"
    },
    "template": {
      "metadata": {
        "generateName": "kyrene-pod-",
        "labels": {
          "service_name": "kyrene",
          "version": "v3"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "kyrene-container",
            "image": "gcr.io/mpj-test-1221/kyrene:1",
            "ports": [
              {
                "containerPort": 3000,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
